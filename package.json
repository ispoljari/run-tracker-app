{
  "name": "run-tracker-app",
  "version": "1.0.0",
  "description": "This app enables users to log their runs, analyse their progress on a weekly basis, and share their posts with other users on the platform",
  "main": "server.js",
  "scripts": {
    "start-dev": "webpack-dev-server --mode development --port 3000 && nodemon server.js",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start-prod": "node server.js",
    "test": "mocha ./app/test/ --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ispoljari/run-tracker-app.git"
  },
  "keywords": [
    "node",
    "express",
    "mongoDB",
    "javascript",
    "running",
    "run"
  ],
  "author": "Ivan Spoljaric",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ispoljari/run-tracker-app/issues"
  },
  "homepage": "https://github.com/ispoljari/run-tracker-app#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "joi": "^14.0.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.6",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.5",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
